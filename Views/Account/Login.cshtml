@model WeedStore.Models.DTOs.UserLoginDTOs

<div class="container">
    <form asp-action="Login" asp-controller="Account" method="post">
        <label asp-for="Email">
            Email
        </label>
        <input asp-for="Email" />
        <span asp-validation-for="Email"></span>
        <label asp-for="Password">
            Password
        </label>
        <input asp-for="Password"/>
        <span asp-validation-for="Password">

        </span>
        <input type="submit" value="Login" />
    </form>
    <p>
        Dont Have an account <a class="nav-link text-dark float-right" asp-area="" asp-controller="Account" asp-action="Register">Register</a>
    </p>
</div>